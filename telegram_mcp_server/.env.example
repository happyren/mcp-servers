# Telegram Bot Token from @BotFather
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Your Telegram user ID or chat ID to receive messages
# You can get your ID by messaging @userinfobot on Telegram
TELEGRAM_CHAT_ID=your_chat_id_here

# Optional: Custom API base URL (default: https://api.telegram.org)
# TELEGRAM_API_BASE_URL=https://api.telegram.org

# Optional: Polling timeout in seconds (default: 30)
# POLLING_TIMEOUT=30
